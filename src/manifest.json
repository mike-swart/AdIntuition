{
  "name": "AdIntuition",
  "version": "1.0",
  "description": "Shows if a content creator on Youtube is making money off of the video based off of a paid partnership with a company",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "tabs", "webNavigation", "notifications", "*://*.youtube.com/*"],
  "browser_action": {
    "default_icon": "logos/logo.png",
    "default_title": "This is a default title",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
